<Window x:Class="MSL.SetServerconfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        xmlns:controls="clr-namespace:MSL.controls"
        xmlns:local="clr-namespace:MSL"
        mc:Ignorable="d"
        Title="MSL-配置文件修改" Height="500" Width="800" MinHeight="500" MinWidth="800" Loaded="Window_Loaded" Closing="Window_Closing" WindowStartupLocation="CenterOwner" Background="{DynamicResource BackgroundBrush}" SizeChanged="Window_SizeChanged" >
    <WindowChrome.WindowChrome>
        <WindowChrome GlassFrameThickness="-1" CaptionHeight="40"/>
    </WindowChrome.WindowChrome>
    <Window.DataContext>
        <controls:WindowViewModel/>
    </Window.DataContext>
    <Grid x:Name="MainGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <DockPanel x:Name="TitleGrid" Grid.Row="0" Background="{DynamicResource SideMenuBrush}">
            <TextBlock x:Name="TitleBox" Text="{Binding Title, RelativeSource={RelativeSource AncestorType={x:Type Window}, Mode=FindAncestor}}" Foreground="{StaticResource TextBlockBrush}" FontSize="14" VerticalAlignment="Center" Margin="10,0,0,0"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" WindowChrome.IsHitTestVisibleInChrome="True">
                <Button x:Name="MinBtn" hc:IconElement.Geometry="{StaticResource WindowMinGeometry}" hc:BorderElement.CornerRadius="30" Height="32" Width="32" Margin="0,0,10,0" BorderBrush="{x:Null}" Foreground="{StaticResource TextBlockBrush}" Command="{Binding MinimizeCommand}" CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Window}}">
                    <Button.Background>
                        <SolidColorBrush Color="White" Opacity="0.1"/>
                    </Button.Background>
                </Button>
                <Button x:Name="MaxBtn" hc:IconElement.Geometry="{StaticResource WindowMaxGeometry}" hc:BorderElement.CornerRadius="30" Height="32" Width="32" Margin="0,0,10,0" BorderBrush="{x:Null}" Foreground="{StaticResource TextBlockBrush}" Command="{Binding MaximizeCommand}" CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Window}}">
                    <Button.Background>
                        <SolidColorBrush Color="White" Opacity="0.1"/>
                    </Button.Background>
                </Button>
                <Button x:Name="CloseBtn" hc:IconElement.Geometry="{StaticResource CloseGeometry}" hc:BorderElement.CornerRadius="30" Height="32" Width="32" Margin="0,0,10,0" BorderBrush="{x:Null}" Foreground="{StaticResource TextBlockBrush}" Command="{Binding CloseCommand}" CommandParameter="{Binding RelativeSource={RelativeSource AncestorType=Window}}">
                    <Button.Background>
                        <SolidColorBrush Color="White" Opacity="0.1"/>
                    </Button.Background>
                </Button>
            </StackPanel>
        </DockPanel>
        <Grid x:Name="BodyGrid" Grid.Row="1">
            <Grid>
                <TabControl Background="{x:Null}">
                    <TabItem x:Name="server" Header="server.properties" Background="{x:Null}">
                        <Grid>
                            <TextBox Margin="10,36,0,10" TextWrapping="Wrap" Text="# By FirstDream&#xA;############&#xA;# 基本设置 #&#xA;############&#xA;# 正版验证&#xA;# 正版或外置登录必须启用&#xA;# BC或Velocity群组服必须禁用&#xA;online-mode=true&#xA;&#xA;# 服务器简介&#xA;motd=A Minecraft Server&#xA;&#xA;# 默认游戏模式&#xA;gamemode=survival&#xA;&#xA;# 玩家加入游戏时自动恢复为默认游戏模式&#xA;force-gamemode=false&#xA;# 默认游戏难度&#xA;difficulty=easy&#xA;&#xA;# 最大玩家数&#xA;max-players=20&#xA;&#xA;# 启用玩家间伤害&#xA;pvp=true&#xA;&#xA;# 隐藏在线玩家列表&#xA;hide-online-players=false&#xA;&#xA;# 玩家超时踢出时间（分钟，0禁用）&#xA;player-idle-timeout=0&#xA;&#xA;# 启用地狱&#xA;allow-nether=true&#xA;&#xA;# 默认世界文件夹名称&#xA;level-name=world&#xA;&#xA;# 出生点保护区域半径（方块，0禁用）&#xA;spawn-protection=16&#xA;&#xA;# 禁用原版反飞行检测&#xA;# 如有玩家被误判飞行请启用此项&#xA;allow-flight=false&#xA;&#xA;# 启用极限模式（玩家死亡后会被自动封禁）&#xA;hardcore=false&#xA;&#xA;############&#xA;# 权限设置 #&#xA;############&#xA;# 启用白名单&#xA;# ⚠️用于离线模式有绕过或不可用风险&#xA;white-list=false&#xA;&#xA;# 启用后刷新、修改白名单会立即踢出不在白名单的玩家&#xA;enforce-whitelist=false&#xA;&#xA;# 启用命令方块&#xA;enable-command-block=false&#xA;&#xA;# 通告RCON命令执行结果到OP&#xA;broadcast-rcon-to-ops=true&#xA;&#xA;# 通告控制台命令执行结果到OP&#xA;broadcast-console-to-ops=true&#xA;&#xA;# OP权限等级&#xA;# 等级越高可使用的命令越多&#xA;op-permission-level=4&#xA;&#xA;# 函数权限等级&#xA;# 等级越高可使用的命令越多&#xA;function-permission-level=2&#xA;&#xA;############&#xA;# 性能设置 #&#xA;############&#xA;# 渲染距离（区块）&#xA;# ⚠️此选项被Spigot继承，可在Spigot世界设置中调整&#xA;# ⚠️极其影响性能和带宽，服务器卡顿可降低此项&#xA;# 对于1.18之前的版本，此选项同时控制区块加载距离&#xA;# 若在Paper世界设置中指定 no-tick-view-distance&#xA;# 则指定后此选项仅控制区块加载距离&#xA;# 对于1.18及更高版本，此选项仅控制渲染距离&#xA;view-distance=10&#xA;&#xA;# 加载距离（区块）&#xA;# ⚠️此选项被Spigot继承，可在Spigot世界设置中调整&#xA;# ⚠️极其影响性能，服务器卡顿可降低此项&#xA;# 仅对1.18及更高版本有效&#xA;simulation-distance=10&#xA;&#xA;# 实体发送距离，值为百分比&#xA;# 降低此值可降低网络使用，但会使远处实体不可见&#xA;entity-broadcast-range-percentage=100&#xA;&#xA;# 生成村民&#xA;spawn-npcs=true&#xA;&#xA;# 生成动物&#xA;spawn-animals=true&#xA;&#xA;# 生成怪物&#xA;spawn-monsters=true&#xA;&#xA;# 最大世界大小&#xA;max-world-size=29999984&#xA;&#xA;# 同步区块IO&#xA;# ⚠️禁用可提高性能，但服务端异常关闭极易损坏区块&#xA;sync-chunk-writes=true&#xA;&#xA;# 服务端超时时间【此选项被Spigot覆盖，此处编辑无效】&#xA;max-tick-time=60000&#xA;&#xA;############&#xA;# 网络设置 #&#xA;############&#xA;# 服务端监听IP，留空监听所有IP&#xA;# ⚠️填写错误将导致服务端无法启动&#xA;server-ip=&#xA;&#xA;# 服务端监听端口&#xA;server-port=25565&#xA;&#xA;# 网络压缩阈值（0全部压缩，-1禁用压缩）&#xA;# 启用压缩可降低网络使用，但会增加CPU使用&#xA;network-compression-threshold=256&#xA;&#xA;# RCON远程控制&#xA;# ⚠️RCON安全性极低，切勿将RCON暴露至公网&#xA;enable-rcon=false&#xA;&#xA;# RCON远程控制监听端口&#xA;rcon.port=25575&#xA;&#xA;# RCON远程控制密码&#xA;rcon.password=&#xA;&#xA;# GameSpy4状态查询&#xA;enable-query=false&#xA;&#xA;# GameSpy4监听端口&#xA;query.port=25565&#xA;&#xA;# 数据包限制（0禁用）&#xA;# ⚠️不建议使用，Paper和ViaVersion有更好的数据包限制实现&#xA;rate-limit=0&#xA;&#xA;############&#xA;# 其它设置 #&#xA;############&#xA;# 资源包下载直链&#xA;resource-pack=&#xA;&#xA;# 资源包说明信息&#xA;resource-pack-prompt=&#xA;&#xA;# 强制使用资源包&#xA;require-resource-pack=false&#xA;&#xA;# 资源包SHA1校验&#xA;resource-pack-sha1=&#xA;&#xA;# 启用调试输出【此选项被Spigot覆盖，此处编辑无效】&#xA;debug=false&#xA;&#xA;use-native-transport=true&#xA;prevent-proxy-connections=false&#xA;text-filtering-config=&#xA;enable-jmx-monitoring=false" IsReadOnly="True" VerticalScrollBarVisibility="Auto" Padding="8,8,8,8" HorizontalAlignment="Left" Width="380"/>
                            <TextBox x:Name="item001" Margin="400,36,10,10" TextWrapping="Wrap" Text="TextBox" VerticalScrollBarVisibility="Auto" Padding="8,8,8,8"/>
                            <Label Content="汉化文件：" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" FontSize="18" Background="{x:Null}" BorderBrush="{x:Null}" Height="32"/>
                            <Label Content="（请参照汉化修改配置文件，修改内容会自动保存）" HorizontalAlignment="Left" Margin="100,5,0,0" VerticalAlignment="Top" Background="{x:Null}" BorderBrush="{x:Null}" Padding="0,5,0,5"/>
                            <Label Content="配置文件：" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="18" BorderBrush="{x:Null}" Background="{x:Null}" Height="32" Margin="400,0,0,0"/>
                        </Grid>
                    </TabItem>
                    <TabItem x:Name="bukkit" Header="bukkit.yml" Background="{x:Null}">
                        <Grid>
                            <TextBox Margin="10,36,0,10" TextWrapping="Wrap" Text="# By FirstDream&#xA;settings:&#xA;  # 启用末地&#xA;  allow-end: true&#xA;  # 在服务器掉刻时发出警告消息&#xA;  warn-on-overload: true&#xA;  permissions-file: permissions.yml&#xA;  update-folder: update&#xA;  plugin-profiling: false&#xA;  connection-throttle: 4000&#xA;  query-plugins: true&#xA;  deprecated-verbose: default&#xA;  # 服务器关闭踢出玩家时的消息&#xA;  shutdown-message: Server closed&#xA;  minimum-api: none&#xA;# 生物生成限制&#xA;spawn-limits:&#xA;  # 怪物&#xA;  monsters: 70&#xA;  # 动物&#xA;  animals: 10&#xA;  # 水生动物&#xA;  water-animals: 5&#xA;  # 基于环境生成的水生动物（如海豚）&#xA;  water-ambient: 20&#xA;  # 位于地表以下的水生生物&#xA;  water-underground-creature: 5&#xA;  # 基于环境生成的生物（蝙蝠）&#xA;  ambient: 15&#xA;chunk-gc:&#xA;  # 已卸载区块回收间隔（tick）&#xA;  period-in-ticks: 600&#xA;# 生物生成尝试速率&#xA;ticks-per:&#xA;  # 动物&#xA;  animal-spawns: 400&#xA;  # 怪物&#xA;  monster-spawns: 1&#xA;  # 水生动物&#xA;  water-spawns: 1&#xA;  # 基于环境生成的水生动物（如海豚）&#xA;  water-ambient-spawns: 1&#xA;  # 位于地表以下的水生生物&#xA;  water-underground-creature-spawns: 1&#xA;  # 基于环境生成的生物（蝙蝠）&#xA;  ambient-spawns: 1&#xA;  # 默认自动保存间隔（tick）&#xA;  # ⚠️此选项可被Paper继承，可在Spigot世界设置中调整&#xA;  autosave: 6000&#xA;aliases: now-in-commands.yml&#xA;" IsReadOnly="True" VerticalScrollBarVisibility="Auto" Padding="8,8,8,8" HorizontalAlignment="Left" Width="380"/>
                            <TextBox x:Name="item002" Margin="400,36,10,10" TextWrapping="Wrap" Text="TextBox" VerticalScrollBarVisibility="Auto" Padding="8,8,8,8"/>
                            <Label Content="汉化文件：" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" FontSize="18" Background="{x:Null}" BorderBrush="{x:Null}" Height="32"/>
                            <Label Content="（请参照汉化修改配置文件，修改内容会自动保存）" HorizontalAlignment="Left" Margin="100,5,0,0" VerticalAlignment="Top" Background="{x:Null}" BorderBrush="{x:Null}" Padding="0,5,0,5"/>
                            <Label Content="配置文件：" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="18" BorderBrush="{x:Null}" Background="{x:Null}" Height="32" Margin="400,0,0,0"/>
                        </Grid>
                    </TabItem>
                    <TabItem x:Name="spigot" Header="spigot.yml" Background="{x:Null}">
                        <Grid>
                            <TextBox Margin="10,36,0,10" TextWrapping="Wrap" Text="# By FirstDream&#xA;settings:&#xA;  # 调试输出&#xA;  debug: false&#xA;  # BungeeCord模式&#xA;  bungeecord: false&#xA;  moved-wrongly-threshold: 0.0625&#xA;  save-user-cache-on-stop-only: false&#xA;  moved-too-quickly-multiplier: 10.0&#xA;  sample-count: 12&#xA;  player-shuffle: 0&#xA;  user-cache-size: 1000&#xA;  # 服务端超时时间，任一游戏刻的执行时间超过此值&#xA;  # 则认为服务端已卡死，将自动强制关闭服务端（秒）&#xA;  timeout-time: 60&#xA;  restart-on-crash: true&#xA;  restart-script: ./start.sh&#xA;  # NettyIO线程数&#xA;  netty-threads: 4&#xA;  # 通告村民死亡信息&#xA;  log-villager-deaths: true&#xA;  # 通告命名实体死亡信息&#xA;  log-named-deaths: true&#xA;  attribute:&#xA;    # 最大生命值&#xA;    maxHealth:&#xA;      max: 2048.0&#xA;    # 最大移动速度&#xA;    movementSpeed:&#xA;      max: 2048.0&#xA;    # 最大攻击伤害&#xA;    attackDamage:&#xA;      max: 2048.0&#xA;# 提示消息&#xA;messages:&#xA;  # 无白名单&#xA;  whitelist: You are not whitelisted on this server!&#xA;  # 未知命令&#xA;  unknown-command: Unknown command. Type “/help“ for help.&#xA;  # 服务器已满&#xA;  server-full: The server is full!&#xA;  # 客户端版本太旧&#xA;  outdated-client: Outdated client! Please use {0}&#xA;  # 客户端版本太新&#xA;  outdated-server: Outdated server! I'm still on {0}&#xA;  # 服务器重启&#xA;  restart: Server is restarting&#xA;advancements:&#xA;  # 禁止保存成就数据&#xA;  disable-saving: false&#xA;  # 禁用成就&#xA;  disabled:&#xA;  - minecraft:story/disabled&#xA;players:&#xA;  # 禁止保存玩家数据&#xA;  disable-saving: false&#xA;# 世界设置&#xA;world-settings:&#xA;  # 世界名称，default默认&#xA;  default:&#xA;    # 允许对已生成的区块扩展世界高度&#xA;    below-zero-generation-in-existing-chunks: true&#xA;    verbose: false&#xA;    # 物品展示框、画、栓绳等的更新时间间隔（刻）&#xA;    hanging-tick-frequency: 100&#xA;    # 允许僵尸搜寻和攻击村民&#xA;    zombie-aggressive-towards-villager: true&#xA;    # 允许下界传送门生成僵尸猪灵&#xA;    enable-zombie-pigmen-portal-spawns: true&#xA;    # 渲染距离，default表示使用server.properties的值（区块）&#xA;    view-distance: default&#xA;    # 模拟距离，default表示使用server.properties的值（区块）&#xA;    simulation-distance: default&#xA;    thunder-chance: 100000&#xA;    merge-radius:&#xA;      # 掉落物品合并半径（方块）&#xA;      item: 2.5&#xA;      # 经验球合并半径（方块）&#xA;      exp: 3.0&#xA;    # 掉落物品消失时间（刻）&#xA;    item-despawn-rate: 6000&#xA;    # 箭消失时间（刻）&#xA;    arrow-despawn-rate: 1200&#xA;    # 三叉戟消失时间（刻）&#xA;    trident-despawn-rate: 1200&#xA;    # 凋灵音效半径（方块）&#xA;    wither-spawn-sound-radius: 0&#xA;    # 末地传送门音效半径（方块）&#xA;    end-portal-sound-radius: 0&#xA;    # 移除刷怪笼生成生物的AI&#xA;    nerf-spawner-mobs: false&#xA;    # 末影龙音效半径（方块）&#xA;    dragon-death-sound-radius: 0&#xA;    # 自然刷怪半径（区块）&#xA;    mob-spawn-range: 8&#xA;    # 植物生长速度调节，100默认&#xA;    growth:&#xA;      cactus-modifier: 100&#xA;      cane-modifier: 100&#xA;      melon-modifier: 100&#xA;      mushroom-modifier: 100&#xA;      pumpkin-modifier: 100&#xA;      sapling-modifier: 100&#xA;      beetroot-modifier: 100&#xA;      carrot-modifier: 100&#xA;      potato-modifier: 100&#xA;      wheat-modifier: 100&#xA;      netherwart-modifier: 100&#xA;      vine-modifier: 100&#xA;      cocoa-modifier: 100&#xA;      bamboo-modifier: 100&#xA;      sweetberry-modifier: 100&#xA;      kelp-modifier: 100&#xA;      twistingvines-modifier: 100&#xA;      weepingvines-modifier: 100&#xA;      cavevines-modifier: 100&#xA;      glowberry-modifier: 100&#xA;    # 标准实体活动半径，位于此半径内的实体全速计算&#xA;    # 对于Paper端，超出此范围的实体以1/4的速度计算&#xA;    # 对于Airplane端，此选项被动态实体活动范围覆盖&#xA;    # 其它服务端有其它不同的行为，请查阅它们的文档&#xA;    entity-activation-range:&#xA;      # 动物&#xA;      animals: 32&#xA;      # 怪物&#xA;      monsters: 32&#xA;      # 袭击实体&#xA;      raiders: 48&#xA;      # 其它实体&#xA;      misc: 16&#xA;      # 水中的实体&#xA;      water: 16&#xA;      # 标准实体行为的村民&#xA;      villagers: 32&#xA;      # 飞行实体（幻翼，蜜蜂，蝙蝠）&#xA;      flying-monsters: 32&#xA;      wake-up-inactive:&#xA;        animals-max-per-tick: 4&#xA;        animals-every: 1200&#xA;        animals-for: 100&#xA;        monsters-max-per-tick: 8&#xA;        monsters-every: 400&#xA;        monsters-for: 100&#xA;        villagers-max-per-tick: 4&#xA;        villagers-every: 600&#xA;        villagers-for: 100&#xA;        flying-monsters-max-per-tick: 8&#xA;        flying-monsters-every: 200&#xA;        flying-monsters-for: 100&#xA;      villagers-work-immunity-after: 100&#xA;      villagers-work-immunity-for: 20&#xA;      villagers-active-for-panic: true&#xA;      # 允许位于标准实体活动半径外的村民行为&#xA;      # 禁用将导致玩家远离后村民无法补货，刷铁机无法工作&#xA;      tick-inactive-villagers: true&#xA;    # 结构种子修改，用于防止地图种子被破解&#xA;    # 将以下数字全部修改为其它任意随机数字&#xA;    # Paper和Purpur端也有相关选项，建议修改&#xA;    seed-village: 10387312&#xA;    seed-desert: 14357617&#xA;    seed-igloo: 14357618&#xA;    seed-jungle: 14357619&#xA;    seed-swamp: 14357620&#xA;    seed-monument: 10387313&#xA;    seed-shipwreck: 165745295&#xA;    seed-ocean: 14357621&#xA;    seed-outpost: 165745296&#xA;    seed-endcity: 10387313&#xA;    seed-slime: 987234911&#xA;    seed-bastion: 30084232&#xA;    seed-fortress: 30084232&#xA;    seed-mansion: 10387319&#xA;    seed-fossil: 14357921&#xA;    seed-portal: 34222645&#xA;    # 实体可视半径，半径外的实体对玩家不可见且不发送数据&#xA;    entity-tracking-range:&#xA;      # 其它玩家&#xA;      players: 48&#xA;      # 动物&#xA;      animals: 48&#xA;      # 怪物&#xA;      monsters: 48&#xA;      # 杂项（物品展示框、画、掉落物品等）&#xA;      misc: 32&#xA;      # 其它&#xA;      other: 64&#xA;    ticks-per:&#xA;      # 漏斗传输物品时间间隔（刻）&#xA;      hopper-transfer: 8&#xA;      # 漏斗检查（掉落物品、容器等）时间间隔（刻）&#xA;      hopper-check: 1&#xA;    # 漏斗每次传输的物品数量&#xA;    hopper-amount: 1&#xA;    # 每刻最大TNT爆炸数量，超出数量的TNT爆炸将被推迟&#xA;    # 无法关闭，用于世吞请设置更大数字&#xA;    max-tnt-per-tick: 100&#xA;    # 玩家饥饿速率调整&#xA;    hunger:&#xA;      # 跳跃&#xA;      jump-walk-exhaustion: 0.05&#xA;      # 跑跳&#xA;      jump-sprint-exhaustion: 0.2&#xA;      # 攻击&#xA;      combat-exhaustion: 0.1&#xA;      # 回血&#xA;      regen-exhaustion: 6.0&#xA;      # 游泳&#xA;      swim-multiplier: 0.01&#xA;      # 疾跑&#xA;      sprint-multiplier: 0.1&#xA;      # 其它&#xA;      other-multiplier: 0.0&#xA;    # 每刻超出此时间的实体和方块实体的行为将被推迟&#xA;    # 对于Paper及其分支端，此选项无任何作用&#xA;    max-tick-time:&#xA;      # 方块实体&#xA;      tile: 50&#xA;      # 实体&#xA;      entity: 50&#xA;commands:&#xA;  # 记录命令方块执行结果到控制台&#xA;  silent-commandblock-console: false&#xA;  # 记录玩家执行的命令到控制台&#xA;  log: true&#xA;  # 使用命令补全时最少需要输入的字符数量&#xA;  tab-complete: 0&#xA;  # 命令补全发送命令前缀，如 minecraft:&#xA;  send-namespaced: true&#xA;  spam-exclusions:&#xA;  - /skill&#xA;  replace-commands:&#xA;  - setblock&#xA;  - summon&#xA;  - testforblock&#xA;  - tellraw&#xA;config-version: 12&#xA;stats:&#xA;  # 禁止保存玩家统计信息&#xA;  disable-saving: false&#xA;  forced-stats: {}&#xA;" IsReadOnly="True" VerticalScrollBarVisibility="Auto" Padding="8,8,8,8" HorizontalAlignment="Left" Width="380"/>
                            <TextBox x:Name="item003" Margin="400,36,10,10" TextWrapping="Wrap" Text="TextBox" VerticalScrollBarVisibility="Auto" Padding="8,8,8,8"/>
